/* components/getshop.wxss */
.m-cover {
   left: 0;
   top: 0;
   right: 0;
   bottom: 0;
   position: fixed;
   z-index: 66;
   background: rgba(0,0,0,0.5);
}
.m-coverCheck {
  width:511rpx;
  background:white;
  border-radius:12rpx;
  left:50%;
  position:absolute;
  transform:translate(-50%,-50%);
  /* height:306rpx; */
  top:50%;
  padding: 20rpx;
}
.m-coverCheck text {
  font-size:26rpx;
}
.m-coverint {
  width:100%;
  /* height:150rpx; */
  padding: 40rpx 0;
}
.m-coverint .section__title {
  font-size: 30rpx;
  display: inline-block;
}
.m-coverint picker {
  font-size:35rpx;
  display:inline-block;
  vertical-align:top;
  border-radius:12rpx;
  border:1px solid #f5f5f5;
  width:70%;
  padding-left:5%;
  height:55rpx;
  line-height:55rpx;
  margin-top:-1%;
  margin-left:4%;
}
.m-cover-affirm {
  width:100%;
  height:79rpx;
  line-height:79rpx;
  color:black;
  font-size:33rpx;
  text-align:center;
}