/* components/mineTab/mineTab.wxss */
@import "../../app.wxss";
.tabbox{
  text-align: center;
  background: white;
}
.tabbox view{
  position: relative;
  padding: 24rpx 0;
  font-size: 30rpx;
}
.tabbox view.active{
  color: #4fd2c2;
}
.tabbox view progress{
  position: absolute;
  width: 30%;
  left: 35%;
  bottom: 0;
  border-radius: 10rpx;
}